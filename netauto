	"
	nmap -sV -p 3299 $IP -oN Netauto/$IP/Protocols/SAPRouter/nmap/version.nmap
	path_check "nmap -sV -p 3299 $IP -oN Netauto/$IP/Protocols/SAPRouter/nmap/version.nmap" "
