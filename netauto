	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_YELLOW}WARNING${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Sudo privileges is required to enumerate Tiller${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Tiller/
	path_check "mkdir -p Netauto/$IP/Protocols/Tiller/" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/Tiller/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/Tiller/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sC -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/script.nmap
	path_check "nmap -sC -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/version.nmap
	path_check "nmap -sV -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sC -sV -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/info.nmap
	path_check "nmap -sC -sV -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	sudo nmap -sS -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/stealth-scan.nmap
	path_check "sudo nmap -sS -p 44134 $IP -oN Netauto/$IP/Protocols/Tiller/nmap/stealth-scan.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

ethernetip_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating EthernetIP on port 44818${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/EthernetIP/
	path_check "mkdir -p Netauto/$IP/Protocols/EthernetIP/" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/EthernetIP/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/EthernetIP/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sC -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/script.nmap
	path_check "nmap -sC -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/version.nmap
	path_check "nmap -sV -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sC -sV -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/info.nmap
	path_check "nmap -sC -sV -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script enip-info -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/info-script.nmap
	path_check "nmap --script enip-info -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/info-script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -n -sV --script enip-info -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/ext-info-script.nmap
	path_check "nmap -n -sV --script enip-info -p 44818 $IP -oN Netauto/$IP/Protocols/EthernetIP/nmap/ext-info-script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

