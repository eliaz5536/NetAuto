x11_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating X11 on port 6000 & in range up to 6005${ENDCOLOR}\n"
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_YELLOW}WARNING${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Sudo privileges is required to enumerate X11${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/X11/
	path_check "mkdir -p Netauto/$IP/Protocols/X11/" "	Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/X11/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/X11/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script x11-access -p 6000 $IP -oN Netauto/$IP/Protocols/X11/nmap/access.nmap
	path_check "nmap --script x11-access -p 6000 $IP -oN Netauto/$IP/Protocols/X11/nmap/access.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV --script x11-access -p 6000 $IP -oN Netauto/$IP/Protocols/X11/nmap/access-version.nmap
	path_check "nmap -sV --script x11-access -p 6000 $IP -oN Netauto/$IP/Protocols/X11/nmap/access-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	sudo nmap -Pn -n --open -p6000-6005 --script=x11-access $IP/24 -oN Netauto/$IP/Protocols/X11/nmap/extended-script.nmap
	path_check "sudo nmap -Pn -n --open -p6000-6005 --script=x11-access $IP/24 -oN Netauto/$IP/Protocols/X11/nmap/extended-script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

ajp_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating AJP on port 8009${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/AJP/
	path_check "mkdir -p Netauto/$IP/Protocols/AJP/" "	Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/AJP/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/AJP/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script ajp-auth -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/auth.nmap
	path_check "nmap --script ajp-auth -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/auth.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ajp-auth --script-args ajp-auth.path=/login -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/auth-login.nmap
	path_check "nmap --script ajp-auth --script-args ajp-auth.path=/login -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/auth-login.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ajp-headers -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/headers.nmap
	path_check "nmap --script ajp-headers -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/headers.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ajp-methods -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/methods.nmap
	path_check "nmap --script ajp-methods -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/methods.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ajp-request -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/request.nmap
	path_check "nmap --script ajp-request -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/request.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV --script ajp-auth,ajp-headers,ajp-methods,ajp-request -n -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/all.nmap
	path_check "nmap -sV --script ajp-auth,ajp-headers,ajp-methods,ajp-request -n -p 8009 $IP -oN Netauto/$IP/Protocols/AJP/nmap/all.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

