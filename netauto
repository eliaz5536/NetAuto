hadoop_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Hadoop on port 50030, 50060, 50070 & 50090${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Hadoop/
	path_check "mkdir -p Netauto/$IP/Protocols/Hadoop/" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/Hadoop/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/Hadoop/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script hadoop-jobtracker-info -p 50030 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/jobtracker-info.nmap
	path_check "nmap --script hadoop-jobtracker-info -p 50030 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/jobtracker-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script hadoop-tasktracker-info -p 50060 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/tasktracker-info.nmap
	path_check "nmap --script hadoop-tasktracker-info -p 50060 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/tasktracker-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script hadoop-namenode-info -p 50070 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/namenode-info.nmap
	path_check "nmap --script hadoop-namenode-info -p 50070 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/namenode-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script hadoop-datanode-info -p 50075 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/datanode-info.nmap
	path_check "nmap --script hadoop-datanode-info -p 50075 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/datanode-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script hadoop-secondary-namenode-info -p 50090 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/secondary-namenode-info.nmap
	path_check "nmap --script hadoop-secondary-namenode-info -p 50090 $IP -oN Netauto/$IP/Protocols/Hadoop/nmap/secondary-namenode-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script "hadoop-* and not brute" -p 50030,50060,50070,50075,50090 -oN Netauto/$IP/Protocols/Hadoop/nmap/all.nmap
	path_check "nmap --script "hadoop-* and not brute" -p 50030,50060,50070,50075,50090 -oN Netauto/$IP/Protocols/Hadoop/nmap/all.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV --script "hadoop-* and not brute" -p 50030,50060,50070,50075,50090 -oN Netauto/$IP/Protocols/Hadoop/nmap/all-ext.nmap
	path_check "nmap -sV --script "hadoop-* and not brute" -p 50030,50060,50070,50075,50090 -oN Netauto/$IP/Protocols/Hadoop/nmap/all-ext.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

http_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating HTTP on port 80${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/HTTP/
	path_check "mkdir -p Netauto/$IP/Protocols/HTTP/" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/HTTP/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/HTTP/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script http-auth --script-args http-auth.path=/login -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/auth-login.nmap
	path_check "nmap --script http-auth --script-args http-auth.path=/login -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/auth-login.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script http-devframework -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/devframework.nmap
	path_check "nmap --script http-devframework -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/devframework.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script http-enum -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/enum.nmap
	path_check "nmap --script http-enum -p 80 $IP -oN Netauto/$IP/Protocols/HTTP/nmap/enum.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
