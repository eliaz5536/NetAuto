	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Portmapper on port 111${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Portmapper/nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Portmapper/nmap" "NMAP Portmapper"

	nmap -sC -p 111 $IP -oN Netauto/$IP/Protocols/Portmapper/script.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Portmapper/script.nmap" "NMAP Portmapper script scan"

	nmap -sV -p 111 $IP -oN Netauto/$IP/Protocols/Portmapper/version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Portmapper/version.nmap" "NMAP Portmapper version scan"

	nmap -sC -sV -p 111 $IP -oN Netauto/$IP/Protocols/Portmapper/service-version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Portmapper/service-version.nmap" "NMAP Portmapper service and version detection scan"

	nmap -sSUC -p111 $IP -oN Netauto/$IP/Protocols/Portmapper/info.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Portmapper/info.nmap" "NMAP Portmapper info scan"

	echo " "
}

ident_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Ident on port 113${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Ident/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/Ident/nmap" "NMAP Ident"

	nmap -sC -p 113	$IP -oN Netauto/$IP/Protocols/Ident/nmap/script.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Ident/nmap/script.nmap" "NMAP Ident script scan"
	
	nmap -sV -p 113	$IP -oN Netauto/$IP/Protocols/Ident/nmap/version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Ident/nmap/version.nmap" "NMAP Ident version scan"

	nmap -sC -sV -p 113 $IP -oN Netauto/$IP/Protocols/Ident/nmap/service-version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/Ident/nmap/service-version.nmap" "NMAP Ident service and version detection scan"

	echo " "
}

ntp_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating NTP on port 123${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/NTP
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP" "NTP"

	mkdir -p Netauto/$IP/Protocols/NTP/nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap" "NMAP NTP"

	nmap -sU --script ntp-info -p 123 $IP -oN Netauto/$IP/Protocols/NTP/nmap/info.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap/info.nmap" "NMAP NTP info scan"

	nmap -sU --script ntp-monlist -p 123 $IP -oN Netauto/$IP/Protocols/NTP/nmap/monlist.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap/monlist.nmap" "NMAP NTP monitor data enumeration scan"

	nmap -sU --script "ntp* and not (dos or brute)" -p 123 $IP -oN Netauto/$IP/Protocols/NTP/nmap/udp-all.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap/udp-all.nmap" "full NMAP NTP scan"

	nmap -sU -sV --script "ntp* and not (dos or brute)" -p 123 $IP -oN Netauto/$IP/Protocols/NTP/nmap/udp-version-all.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap/udp-version-all.nmap" "full NMAP NTP version scan"

	nmap -sU -pU:123 -Pn -n --script=ntp-monlist $IP -oN Netauto/$IP/Protocols/NTP/nmap/aggressive-monlist.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/nmap/aggressive-monlist.nmap" "NMAP NTP aggressive monitor data enumeration scan"

	mkdir -p Netauto/$IP/Protocols/NTP/ntpq
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/ntpq" "NTPQ"

	ntpq -c readlist $IP | tee Netauto/$IP/Protocols/NTP/ntpq/readlist.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/ntpq/readlist.txt" "NTP NTPQ system variables enumeration"

	ntpq -c readvar $IP | tee Netauto/$IP/Protocols/NTP/ntpq/readvar.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/ntpq/readvar.txt" "NTP NTPQ specified peer variables enumeration"

	ntpq -c peers $IP | tee Netauto/$IP/Protocols/NTP/ntpq/peers.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/ntpq/peers.txt" "NTP NTPQ in-specc peers enumeration"

	ntpq -c associations $IP | tee Netauto/$IP/Protocols/NTP/ntpq/associations.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NTP/ntpq/associations.txt" "NTP NTPQ association identifiers enumeration"

	echo " "
}

msrpc_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating MSRPC on port 135${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/MSRPC/nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/nmap" "NMAP MSRPC"

	nmap -sC -p 135	$IP -oN Netauto/$IP/Protocols/MSRPC/nmap/script.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/nmap/script.nmap" "NMAP MSRPC script scan"

	nmap -sV -p 135	$IP -oN Netauto/$IP/Protocols/MSRPC/nmap/version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/nmap/version.nmap" "NMAP MSRPC version scan"

	nmap -sC -sV -p 135 $IP -oN Netauto/$IP/Protocols/MSRPC/nmap/service-version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/nmap/service-version.nmap" "NMAP MSRPC service and version detection scan"

	nmap --script msrpc-enum -p 135 $IP -oN Netauto/$IP/Protocols/MSRPC/nmap/enum.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/nmap/enum.nmap" "NMAP MSRPC enumeration scan"

	mkdir -p Netauto/$IP/Protocols/MSRPC/rpcdump
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/rpcdump" "RPCDUMP"

	impacket-rpcdump -port 135 $IP | tee Netauto/$IP/Protocols/MSRPC/rpcdump/impacket-rpcdump.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/rpcdump/impacket-rpcdump.txt" "RPC endpoints enumeration"

	/usr/share/doc/python3-impacket/examples/rpcdump.py $IP -p 135 | tee Netauto/$IP/Protocols/MSRPC/rpcdump/rpcdump_port.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/rpcdump/rpcdump_port.txt" "RPC endpoint ports enumeration"

	/usr/share/doc/python3-impacket/examples/rpcmap.py 'ncacn_ip_tcp:$IP' | tee Netauto/$IP/Protocols/MSRPC/rpcdump/rpcmap.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/rpcdump/rpcmap.txt" "RPC mappings enumeration"

	python /opt/tools/MSRPC/IOXIDResolver.py -t $IP | tee Netauto/$IP/Protocols/MSRPC/rpcdump/IOXIDResolver.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/MSRPC/rpcdump/IOXIDResolver.txt" "RPC mapping and ip address interface enumeration"

	echo " "
}

netbios_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating NetBIOS on port 135, 137, 138, 139 & 445${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/NetBios/nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap" ""

	nmap -sU -p 137,138,139 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/udp.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/udp.nmap" "NMAP NetBios UDP scan"

	nmap -vv --reason -Pn -sV -p139,445 --script="(nbstat or smb* or ssl*) and not (brute or broadcast or dos or external or fuzzer)" --script-args="unsafe=1" $IP -oN Netauto/$IP/Protocols/NetBios/nmap/aggressive-udp.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/aggressive-udp.nmap" "NMAP NetBios aggressive UDP scan"
	
	nmap -sT -p 137,139,445 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/tcp.nmap
	path_check "nmap -sT -p 137,139,445 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/tcp.nmap" "NMAP NetBios TCP scan"
	
	nmap -vv --reason -Pn -sU -sV -p137 --script="(nbstat or smb* or ssl*) and not (brute or broadcast or dos or external or fuzzer)" --script-args="unsafe=1" $IP -oN Netauto/$IP/Protocols/NetBios/nmap/aggressive-tcp.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/aggressive-tcp.nmap" "NMAP NetBios aggressive TCP scan"
	
	nmap -sC -p 137,139,445 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/script.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/script.nmap" "NMAP NetBios script scan"
	
	nmap -sV -p 137,139,445 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/version.nmap" "NMAP NetBios version scan"
	
	nmap -sC -sV -p 135,137,139,445 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/service-version.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/script-and-version.nmap" "NMAP NetBios service and version detection scan"
	
	nmap --script broadcast-netbios-master-browser -p 135,137,139 $IP -oN Netauto/$IP/Protocols/NetBios/nmap/broadcast-netbios-master-browser.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/broadcast-netbios-master-browser.nmap" "NMAP NetBios broadcast netbios master browser enumeration scan"
	
	sudo nmap -sU -sV -T4 --script nbstat.nse -p137 -Pn -n $IP -oN Netauto/$IP/Protocols/NetBios/nmap/nbstat.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmap/nbstat.nmap" "NMAP NetBios nbstat enumeration scan"

	mkdir -p Netauto/$IP/Protocols/NetBios/nmblookup
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmblookup" "nmblookup"
	
	nmblookup -A $IP | tee Netauto/$IP/Protocols/NetBios/nmblookup/nmblookup.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nmblookup/nmblookup.txt" "Node status query enumeration"

	mkdir -p Netauto/$IP/Protocols/NetBios/nbtscan
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nbtscan" "nbtscan"
	
	nbtscan $IP | tee Netauto/$IP/Protocols/NetBios/nbtscan/default.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nbtscan/default.txt" "default NetBIOS enumeration scan"
	
	nbtscan -rvh $IP | tee Netauto/$IP/Protocols/NetBios/nbtscan/verbose.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nbtscan/verbose.txt" "verbose NetBIOS enumeration scan"
	
	nbtscan $IP/30 | tee Netauto/$IP/Protocols/NetBios/nbtscan/range-30.txt # Command freezes
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/nbtscan/range-30.txt" "default NetBIOS ip range of 30 enumeration scan"

	nbtscan -r $IP/24 | tee Netauto/$IP/Protocols/NetBios/nbtscan/range-24.txt
	path_check "$(pwd)/tee Netauto/$IP/Protocols/NetBios/nbtscan/range-24.txt" "verbose NetBIOS ip range of 24 enumeration scan"

	mkdir -p Netauto/$IP/Protocols/NetBios/enum4linux
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/enum4linux" "enum4linux"
	
	enum4linux -a -M -l -d $IP | tee Netauto/$IP/Protocols/NetBios/enum4linux/netbios-enum4linux.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/enum4linux/enum4linux.txt" "verbose enum4linux NetBIOS enumeration scan"

	mkdir -p Netauto/$IP/Protocols/NetBios/rpcclient
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/rpcclient" "rpcclient"
	
	rpcclient -U "" -P $IP | tee Netauto/$IP/Protocols/NetBios/rpcclient/null_session_attempt.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/NetBios/rpcclient/null_session_attempt.txt" "null session attempt rpcclient enumeration"

	echo " "
}

smb_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating SMB on port 445${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/SMB/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/SMB/nmap" ""

	nmap --script smb-enum-shares.nse,smb-enum-users.nse -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/enum-scripts.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/enum-scripts.nmap" ""

	nmap --script smb-enum* -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/enum-all.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/enum-all.nmap" ""

	nmap --script smb-protocols -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/protocols.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/protocols.nmap" ""

	nmap --script smb-flood -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/flood.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/flood.nmap" ""

	nmap --script smb-ls -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/list.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/list.nmap" ""

	nmap --script smb-mbenum -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/mbenum.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/mbenum.nmap" ""

	nmap --script smb-os-discovery -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/os-discovery.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/os-discovery.nmap" ""

	nmap --script smb-psexec -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/psexec.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/psexec.nmap" ""

	nmap --script smb-security-mode -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/security-mode.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/security-mode.nmap" ""

	nmap --script smb-server-stats -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/server-stats.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/server-stats.nmap" ""

	nmap --script smb-system-info -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/system-info.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/system-info.nmap" ""

	nmap --script smb-webexec-exploit -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/webexec-exploit.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/webexec-exploit.nmap" ""

	nmap --script smb-double-pulsar-backdoor -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/double-pulsar-backdoor.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/double-pulsar-backdoor.nmap" ""

	nmap --script smb-vuln* -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/vuln-all.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/vuln-all.nmap" ""

	nmap --script "smb-* and not brute" -p 445 $IP -oN Netauto/$IP/Protocols/SMB/nmap/all.nmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmap/all.nmap" ""

	mkdir -p Netauto/$IP/Protocols/SMB/nmblookup
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmblookup" ""

	nmblookup -A $IP | tee Netauto/$IP/Protocols/SMB/nmblookup/smb-nmblookup.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nmblookup/smb-nmblookup.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/nbtscan
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nbtscan" ""

	nbtscan $IP | tee Netauto/$IP/Protocols/SMB/nbtscan/smb-nbtscan.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nbtscan/smb-nbtscan.txt" ""

	nbtscan -r $IP/24 | tee Netauto/$IP/Protocols/SMB/nbtscan/smb-nbtscan-recursive.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/nbtscan/smb-nbtscan-recursive.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/enum4linux
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux" ""

	enum4linux $IP | tee Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux.txt" ""

	enum4linux -a $IP | tee Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-all.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-all.txt" ""

	enum4linux -v $IP | tee Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-verbose.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-verbose.txt" ""

	enum4linux -a -M -l -d $IP | tee Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-extended.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux/smb-enum4linux-extended.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/enum4linux-ng
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux-ng" ""
	
	enum4linux-ng -A $IP | tee Netauto/$IP/Protocols/SMB/enum4linux-ng/smb-enum4linux-ng-all.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux-ng/smb-enum4linux-ng-all.txt" ""

	enum4linux-ng -As $IP | tee Netauto/$IP/Protocols/SMB/enum4linux-ng/smb-enum4linux-short-all.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/enum4linux-ng/smb-enum4linux-short-all.txt" ""
	
	mkdir -p Netauto/$IP/Protocols/SMB/netexec
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec" ""

	netexec smb $IP/24 | tee Netauto/$IP/Protocols/SMB/netexec/smb-netexec-scan.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec/smb-netexec-scan.txt" ""

	netexec smb $IP | tee Netauto/$IP/Protocols/SMB/netexec/smb-netexec-default.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec/smb-netexec-default.txt" ""

	netexec smb $IP-1 $IP-2 | tee Netauto/$IP/Protocols/SMB/netexec/smb-netexec-targetips.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec/smb-netexec-targetips.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/smbmap
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbmap" ""

	smbmap -H $IP | tee Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-scan-and-null-user.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-scan-and-null-user.txt" ""

	smbmap -H $IP -P 445 | tee Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-port-null-user.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-port-null-user.txt" ""

	smbmap -u null -p "" $IP -P 445 | tee Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-null-session.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-null-session.txt" ""

	smbmap -u null -p "" $IP -P 445 -x "whoami" | tee Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-command-execution.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbmap/smb-smbmap-command-execution.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/crackmapexec
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/crackmapexec" ""

	crackmapexec smb $IP -u '' -p '' --shares | tee Netauto/$IP/Protocols/SMB/crackmapexec/null-user-shares.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/crackmapexec/null-user-shares.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/smbclient
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbclient" ""

	smbclient -L $IP | tee Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-directories.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-directories.txt" ""

	smbclient -N -L $IP | tee Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-directories-nopswd.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-directories-nopswd.txt" ""

	smbclient -L\\ -N -I $IP | tee Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-null-session.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-null-session.txt" ""

	smbclient --no-pass -L //$IP | tee Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-null-user.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/smbclient/smb-smbclient-null-user.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/netexec
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec" ""

	netexec smb $IP -u '' -p '' --shares | tee Netauto/$IP/Protocols/SMB/netexec/smb-netexec-nullsessions.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/netexec/smb-netexec-nullsessions.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/rpcclient
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/rpcclient" ""
	
	rpcclient -U "" -N $IP | tee Netauto/$IP/Protocols/SMB/rpcclient/smb-rpcclient-nullsessions.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/rpcclient/smb-rpcclient-nullsessions.txt" ""

	mkdir -p Netauto/$IP/Protocols/SMB/RID-Cycling-Attack
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/RID-Cycling-Attack" ""

	impacket-lookupsid example.local/anonymous@$IP 20000 -no-pass | tee Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-anon-nopass.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-anon-nopass.txt" ""

	impacket-lookupsid example.local/guest@$IP 20000 -no-pass | tee Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-guest-nopass.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-guest-nopass.txt" ""

	impacket-lookupsid example.local/guest@$IP 20000 | tee Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-guest.txt
	path_check "$(pwd)/Netauto/$IP/Protocols/SMB/RID-Cycling-Attack/impacket-looksupid-guest.txt" ""

	echo " "
}

imap_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating IMAP on port 143 & 993${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/IMAP
	path_check "mkdir -p Netauto/$IP/Protocols/IMAP" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/IMAP/nc
	path_check "mkdir -p Netauto/$IP/Protocols/IMAP/nc" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nc $IP 143 | tee Netauto/$IP/Protocols/IMAP/nc/imap-nc.txt
	path_check "nc $IP 143 | tee Netauto/$IP/Protocols/IMAP/nc/imap-nc.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nc -vn $IP 143 | tee Netauto/$IP/Protocols/IMAP/nc/imap-vn.txt
	path_check "nc -vn $IP 143 | tee Netauto/$IP/Protocols/IMAP/nc/imap-vn.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/IMAP/openssl
	path_check "mkdir -p Netauto/$IP/Protocols/IMAP/openssl" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	openssl s_client -connect $IP:993 | tee Netauto/$IP/Protocols/IMAP/openssl/imap-openssl.txt
	path_check "openssl s_client -connect $IP:993 | tee Netauto/$IP/Protocols/IMAP/openssl/imap-openssl.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	openssl s_client -connect $IP:993 -quiet | tee Netauto/$IP/Protocols/IMAP/openssl/imap-openssl-quiet.txt
	path_check "openssl s_client -connect $IP:993 -quiet | tee Netauto/$IP/Protocols/IMAP/openssl/imap-openssl-quiet.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/IMAP/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/IMAP/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script imap-capabilities -p 143,993 $IP -oN Netauto/$IP/Protocols/IMAP/nmap/capabilities.nmap
	path_check "nmap --script imap-capabilities -p 143,993 $IP -oN Netauto/$IP/Protocols/IMAP/nmap/capabilities.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -p 143,993 --script imap-ntlm-info $IP -oN Netauto/$IP/Protocols/IMAP/nmap/ntlm-info-ports.nmap
	path_check "nmap -p 143,993 --script imap-ntlm-info $IP -oN Netauto/$IP/Protocols/IMAP/nmap/ntlm-info-ports.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script imap-ntlm-info -p 143,993 $IP -oN Netauto/$IP/Protocols/IMAP/nmap/ntlm-info.nmap
	path_check "nmap --script imap-ntlm-info -p 143,993 $IP -oN Netauto/$IP/Protocols/IMAP/nmap/ntlm-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

irc_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating IRC on port 194 & from 6660 to 7000${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/IRC
	path_check "mkdir -p Netauto/$IP/Protocols/IRC" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/IRC/nc
	path_check "mkdir -p Netauto/$IP/Protocols/IRC/nc" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nc $IP 194 | tee Netauto/$IP/Protocols/IRC/nc/irc-nc-194.txt
	path_check "nc $IP 194 | tee Netauto/$IP/Protocols/IRC/nc/irc-nc-194.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nc -vn $IP 194 | tee Netauto/$IP/Protocols/IRC/nc/irc-vn-194.txt
	path_check "nc -vn $IP 194 | tee Netauto/$IP/Protocols/IRC/nc/irc-vn-194.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/IRC/openssl
	path_check "mkdir -p Netauto/$IP/Protocols/IRC/openssl" "	Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	openssl s_client -connect $IP:194 -quiet | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-194-quiet.txt
	path_check "openssl s_client -connect $IP:194 -quiet | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-194-quiet.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	openssl s_client -connect $IP:194 | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-194-quiet.txt
	path_check "openssl s_client -connect $IP:194 | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-194-quiet.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	openssl s_client -connect $IP:6667 -quiet | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-6667-quiet.txt
	path_check "openssl s_client -connect $IP:6667 -quiet | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-6667-quiet.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	openssl s_client -connect $IP:6667 | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-6667.txt
	path_check "openssl s_client -connect $IP:6667 | tee Netauto/$IP/Protocols/IRC/openssl/irc-openssl-6667.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/IRC/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/IRC/nmap" "	Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"
	nmap --script irc-info,irc-botnet-channels,irc-unrealircd-backdoor -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/info.nmap
	path_check "nmap --script irc-info,irc-botnet-channels,irc-unrealircd-backdoor -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV --script irc-info,irc-botnet-channels,irc-unrealircd-backdoor -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/info-version.nmap
	path_check "nmap -sV --script irc-info,irc-botnet-channels,irc-unrealircd-backdoor -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/info-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	"
	nmap -sV --script "irc-* and not brute" -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/all.nmap
	path_check "nmap -sV --script "irc-* and not brute" -p 194,6660-7000 $IP -oN Netauto/$IP/Protocols/IRC/nmap/all.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

checkpoint_firewall_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Checkpoint Firewall on port 264${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Checkpoint_Firewall-1
	path_check "mkdir -p Netauto/$IP/Protocols/Checkpoint_Firewall-1" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sC -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/script.nmap
	path_check "nmap -sC -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/version.nmap
	path_check "nmap -sV -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sC -sV -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/script-and-version.nmap
	path_check "nmap -sC -sV -p 264 -oN Netauto/$IP/Protocols/Checkpoint_Firewall-1/nmap/script-and-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

ldap_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating LDAP on port 389${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/LDAP
	path_check "mkdir -p Netauto/$IP/Protocols/LDAP" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"
	
	mkdir -p Netauto/$IP/Protocols/LDAP/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/LDAP/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script ldap-brute --script-args ldap.base='"cn=users,dc=cqure,dc=net"' -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/brute.nmap
	path_check "nmap --script ldap-brute --script-args ldap.base='"cn=users,dc=cqure,dc=net"' -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/brute.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ldap-search -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/search.nmap
	path_check "nmap --script ldap-search -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/search.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ldap-novell-getpass -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/novell-getpass.nmap
	path_check "nmap --script ldap-novell-getpass -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/novell-getpass.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	"
	nmap --script ldap-rootdse -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/rootdse.nmap
	path_check "nmap --script ldap-rootdse -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/rootdse.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	"
	nmap --script "ldap* and not brute" -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/no-brute.nmap
	path_check "nmap --script "ldap* and not brute" -p 389 $IP -oN Netauto/$IP/Protocols/LDAP/nmap/no-brute.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -n -sV --script "ldap* and not brute" $IP -oN Netauto/$IP/Protocols/LDAP/nmap/no-brute-version.nmap
	path_check "nmap -n -sV --script "ldap* and not brute" $IP -oN Netauto/$IP/Protocols/LDAP/nmap/no-brute-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ldap-* $IP -oN Netauto/$IP/Protocols/LDAP/nmap/all.nmap
	path_check "nmap --script ldap-* $IP -oN Netauto/$IP/Protocols/LDAP/nmap/all.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/LDAP/ldapsearch
	path_check "mkdir -p Netauto/$IP/Protocols/LDAP/ldapsearch" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	ldapsearch -LLL -x -H ldap://$IP -b ‘’ -s base '(objectclass=*)' | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-anonymous-credential-dumping.txt
	path_check "ldapsearch -LLL -x -H ldap://$IP -b ‘’ -s base '(objectclass=*)' | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-anonymous-credential-dumping.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	ldapsearch -h $IP -x -s base namingcontexts | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-namingcontexts.txt
	path_check "ldapsearch -h $IP -x -s base namingcontexts | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-namingcontexts.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	ldapsearch -h $IP -x -b "DC=BLACKFIELD,DC=local" | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-domainname.txt
	path_check "ldapsearch -h $IP -x -b "DC=BLACKFIELD,DC=local" | tee Netauto/$IP/Protocols/LDAP/ldapsearch/ldap-domainname.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

ipsec_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating IPSEC on port 500${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/IKE
	path_check "mkdir -p Netauto/$IP/Protocols/IKE" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Protocols/IKE/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/IKE/nmap" "	Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sU -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/udp.nmap
	path_check "nmap -sU -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/udp.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ike-version -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/version.nmap
	path_check "nmap --script ike-version -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script http-cisco-anyconnect -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/http-cisco-anyconnect.nmap
	path_check "nmap --script http-cisco-anyconnect -p 500 $IP -oN Netauto/$IP/Protocols/IKE/nmap/http-cisco-anyconnect.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Protocols/IKE/ike-scan
	path_check "mkdir -p Netauto/$IP/Protocols/IKE/ike-scan" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	
	"

	ike-scan -M $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan.txt
	path_check "ike-scan -M $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	ike-scan -M --showbackoff $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan-aggressive.txt
	path_check "ike-scan -M --showbackoff $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan-aggressive.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	ike-scan -P -M -A -n fakeID $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan-fakeID.txt
	path_check "ike-scan -P -M -A -n fakeID $IP | tee Netauto/$IP/Protocols/IKE/ike-scan/ike-scan-fakeID.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

modbus_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Modbus on port 502${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Protocols/Modbus
	path_check "mkdir -p Netauto/$IP/Protocols/Modbus" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"
	
	mkdir -p Netauto/$IP/Protocols/Modbus/nmap
	path_check "mkdir -p Netauto/$IP/Protocols/Modbus/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sC -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/script.nmap
	path_check "nmap -sC -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/version.nmap
	path_check "nmap -sV -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/version.nmap" "
	
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sC -sV -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/script-and-version.nmap
	path_check "nmap -sC -sV -p 502 -oN Netauto/$IP/Protocols/Modbus/nmap/script-and-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script modbus-discover -p 502 $IP -oN Netauto/$IP/Protocols/Modbus/nmap/discover.nmap
	path_check "nmap --script modbus-discover -p 502 $IP -oN Netauto/$IP/Protocols/Modbus/nmap/discover.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script modbus-discover --script-args modbus-discover.aggressive=true -p 502 $IP -oN Netauto/$IP/Protocols/Modbus/nmap/aggressive-discover.nmap
	path_check "nmap --script modbus-discover --script-args modbus-discover.aggressive=true -p 502 $IP -oN Netauto/$IP/Protocols/Modbus/nmap/aggressive-discover.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

