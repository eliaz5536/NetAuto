	"

	echo " "
}

mssql_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating MSSQL on port 1433${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Databases/MSSQL
	path_check "mkdir -p Netauto/$IP/Databases/MSSQL" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Databases/MSSQL/nmap
	path_check "mkdir -p Netauto/$IP/Databases/MSSQL/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script broadcast-ms-sql-discover -p 1433 $IP  -oN Netauto/$IP/Databases/MSSQL/nmap/broadcast-ms-sql-discover.nmap
	path_check "nmap --script broadcast-ms-sql-discover -p 1433 $IP  -oN Netauto/$IP/Databases/MSSQL/nmap/broadcast-ms-sql-discover.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-info -p 1433 $IP  -oN Netauto/$IP/Databases/MSSQL/nmap/info.nmap
	path_check "nmap --script ms-sql-info -p 1433 $IP  -oN Netauto/$IP/Databases/MSSQL/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-config -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/config.nmap
	path_check "nmap --script ms-sql-config -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/config.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-dac -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/dac.nmap
	path_check "nmap --script ms-sql-dac -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/dac.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-dump-hashes -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/dump-hashes.nmap
	path_check "nmap --script ms-sql-dump-hashes -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/dump-hashes.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	"
	nmap --script ms-sql-empty-password -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/empty-password.nmap
	path_check "nmap --script ms-sql-empty-password -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/empty-password.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-hasdbaccess -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/hasdbaccess.nmap
	path_check "nmap --script ms-sql-hasdbaccess -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/hasdbaccess.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-ntlm-info -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/ntlm-info.nmap
	path_check "nmap --script ms-sql-ntlm-info -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/ntlm-info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-query -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/query.nmap
	path_check "nmap --script ms-sql-query -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/query.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-tables -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/tables.nmap
	path_check "nmap --script ms-sql-tables -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/tables.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-empty-password,ms-sql-xp-cmdshell -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/cmdshell.nmap
	path_check "nmap --script ms-sql-empty-password,ms-sql-xp-cmdshell -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/cmdshell.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script "ms-sql-* and not brute" -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/all.nmap
	path_check "nmap --script "ms-sql-* and not brute" -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/all.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script ms-sql-info,ms-sql-empty-password,ms-sql-xp-cmdshell,ms-sql-config,ms-sql-ntlm-info,ms-sql-tables,ms-sql-hasdbaccess,ms-sql-dac,ms-sql-dump-hashes --script-args mssql.instance-port=1433,mssql.username=sa,mssql.password=,mssql.instance-name=MSSQLSERVER -sV -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/all-ext.nmap
	path_check "nmap --script ms-sql-info,ms-sql-empty-password,ms-sql-xp-cmdshell,ms-sql-config,ms-sql-ntlm-info,ms-sql-tables,ms-sql-hasdbaccess,ms-sql-dac,ms-sql-dump-hashes --script-args mssql.instance-port=1433,mssql.username=sa,mssql.password=,mssql.instance-name=MSSQLSERVER -sV -p 1433 $IP -oN Netauto/$IP/Databases/MSSQL/nmap/all-ext.nmap" "
	
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	" "
	
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	"
	
	echo " "
}

postgresql_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating PostgreSQL on port 5432${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Databases/PostgreSQL
	path_check "mkdir -p Netauto/$IP/Databases/PostgreSQL" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Databases/PostgreSQL/nmap
	path_check "mkdir -p Netauto/$IP/Databases/PostgreSQL/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap -sC -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/script.nmap
	path_check "nmap -sC -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/script.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sV -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/version.nmap
	path_check "nmap -sV -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap -sC -sV -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/info.nmap
	path_check "nmap -sC -sV -p 5432 $IP -oN Netauto/$IP/Databases/PostgreSQL/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

redis_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating Redis on port 6379${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Databases/Redis
	path_check "mkdir -p Netauto/$IP/Databases/Redis" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Databases/Redis/nc
	path_check "mkdir -p Netauto/$IP/Databases/Redis/nc" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nc $IP 6379 | tee Netauto/$IP/Databases/Redis/nc/redis_nc.txt
	path_check "nc $IP 6379 | tee Netauto/$IP/Databases/Redis/nc/redis_nc.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nc -nv $IP 6379 | tee Netauto/$IP/Databases/Redis/nc/redis_nc.txt
	path_check "nc -nv $IP 6379 | tee Netauto/$IP/Databases/Redis/nc/redis_nc.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Databases/Redis/nmap
	path_check "mkdir -p Netauto/$IP/Databases/Redis/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script redis-info -p 6379 $IP -oN Netauto/$IP/Databases/Redis/nmap/info.nmap
	path_check "nmap --script redis-info -p 6379 $IP -oN Netauto/$IP/Databases/Redis/nmap/info.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script redis-info -sV -p 6379 $IP -oN Netauto/$IP/Databases/Redis/nmap/info-ext.nmap
	path_check "nmap --script redis-info -sV -p 6379 $IP -oN Netauto/$IP/Databases/Redis/nmap/info-ext.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	mkdir -p Netauto/$IP/Databases/Redis/redis-cli
	path_check "mkdir -p Netauto/$IP/Databases/Redis/redis-cli" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	redis-cli -h $IP | tee Netauto/$IP/Databases/Redis/redis-cli/redis-cli.txt
	path_check "redis-cli -h $IP | tee Netauto/$IP/Databases/Redis/redis-cli/redis-cli.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

oracledb_scan() {
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Enumerating OracleDB on port 1521${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/Databases/OracleDB
	path_check "mkdir -p Netauto/$IP/Databases/OracleDB" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	mkdir -p Netauto/$IP/Databases/OracleDB/nmap
	path_check "mkdir -p Netauto/$IP/Databases/OracleDB/nmap" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	nmap --script oracle-tns-version -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/tns-version.nmap
	path_check "nmap --script oracle-tns-version -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/tns-version.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script oracle-sid-brute -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/sid-brute.nmap
	path_check "nmap --script oracle-sid-brute -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/sid-brute.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	nmap --script oracle-enum-users -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/enum-users.nmap
	path_check "nmap --script oracle-enum-users -p 1521 $IP -oN Netauto/$IP/Databases/OracleDB/nmap/enum-users.nmap" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Databases/OracleDB/oscanner
	path_check "mkdir -p Netauto/$IP/Databases/OracleDB/oscanner" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	oscanner -s $IP -P 1521 | tee Netauto/$IP/Databases/OracleDB/oscanner/oracledb_oscanner.txt
	path_check "oscanner -s $IP -P 1521 | tee Netauto/$IP/Databases/OracleDB/oscanner/oracledb_oscanner.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	mkdir -p Netauto/$IP/Databases/OracleDB/tnscmd10g
	path_check "mkdir -p Netauto/$IP/Databases/OracleDB/tnscmd10g" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	tnscmd10g version -h $IP | tee Netauto/$IP/Databases/OracleDB/tnscmd10g/oracledb_tnscmd10g.txt
	path_check "tnscmd10g version -h $IP | tee Netauto/$IP/Databases/OracleDB/tnscmd10g/oracledb_tnscmd10g.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	mkdir -p Netauto/$IP/Databases/OracleDB/ODAT
	path_check "mkdir -p Netauto/$IP/Databases/OracleDB/ODAT" "Created DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP" "
	
	Failed to create DHCP protocol output directory of target live host under '$(pwd)/Netauto/$IP/Protocols/DHCP'
	"

	odat sidguesser -s $IP | tee Netauto/$IP/Databases/OracleDB/ODAT/oracledb_odat.txt
	path_check "odat sidguesser -s $IP | tee Netauto/$IP/Databases/OracleDB/ODAT/oracledb_odat.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"
	odat-libc2.12-x86_64 passwordguesser -s $IP -d XE --accounts-file accounts/default.txt | tee Netauto/$IP/Databases/OracleDB/ODAT/passwordguesser.txt
	path_check "odat-libc2.12-x86_64 passwordguesser -s $IP -d XE --accounts-file accounts/default.txt | tee Netauto/$IP/Databases/OracleDB/ODAT/passwordguesser.txt" "
	Generated output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	" "
	Failed to generate output data of NMAP DHCP discover scan under '$(pwd)/Netauto/Protocols/DHCP/nmap/dhcp_discover.txt'
	
	"

	echo " "
}

###############################################################
# Port Detection                                              #
###############################################################
handle_ports() {
	echo -e "${GRAY}------------------------------------ Full Port Scan -------------------------------------${ENDCOLOR}"

	DEFAULT_DIRECTORY=Netauto/$IP/Protocols/

	local ports=$1

	IFS=',' read -ra port_array <<< "$ports"
	echo "$IFS"

	for port in "${port_array[@]}"; do
		case "$port" in
			7) echo_scan ;;
			21) ftp_scan ;;
			22) ssh_scan ;;
			23) telnet_scan ;;
			25) smtp_scan ;;
			465) smtp_scan ;;
			587) smtp_scan ;;
			43) whois_scan ;;
			49) tacacs_scan ;;
			53) dns_scan ;;
			69) tftp_scan ;;
			79) finger_scan ;;
			88) kerberos_scan ;;
			110) pop_scan ;;
			111) portmapper_scan ;;
			113) ident_scan ;;
			123) ntp_scan ;;
			135) msrpc_scan ;;
			593) msrpc_scan ;;
			137) netbios_scan ;;
			138) netbios_scan ;;
			139) smb_scan ;;
			445) smb_scan ;;
			143) imap_scan ;;
			993) imap_scan ;;
			161) snmp_scan ;;
			162) snmp_scan ;;
			10161) snmp_scan ;;
			10162) snmp_scan ;;
			194) irc_scan ;;
			264) check_point_firewall_scan ;;
			389) ldap_scan ;;
			636) ldap_scan ;;
			3268) ldap_scan ;;
			3269) ldap_scan ;;
			500) ipsec_scan ;;
			502) modbus_scan ;;
			512) rexec_scan ;;
			513) rlogin_scan ;;
			514) rsh_scan ;;
			515) lpd_scan ;;
			548) afp_scan ;;
			554) rtsp_scan ;;
			8554) rtsp_scan ;;
			623) ipmi_scan ;;
			631) ipp_scan ;;
			700) epp_scan ;;
			873) rsync_scan ;;
			1026) rusersd_scan ;;
			1080) socks_scan ;;
			1098) java_rmi_scan ;;
			1099) java_rmi_scan ;;
			1050) java_rmi_scan ;;
			1414) ibm_mq_scan ;;
			1521) oracledb_scan ;;
			1723) pptp_scan ;;
			1883) mqtt_scan ;;
			2049) nfs_scan ;;
			2301) compaq_scan ;;
			2381) compaq_scan ;;
			2375) docker_scan ;;
			2376) docker_scan ;;
			3128) squid_scan ;; 
			3260) iscsi_scan ;;
			3289) rdp_scan ;;
			3299) saprouter_scan ;;
			3632) distcc_scan ;;
			3690) subversion_scan ;;
			3702) ws-discovery_scan ;;
			4369) epmd_scan ;;
			4786) cisco_smart_install_scan ;;
			4840) opc_scan ;;
			5000) docker_registry_scan ;;
			5353) mdns_scan ;;
			5439) redshift_scan ;;
			5555) android_debug_bridge_scan ;;
			5601) kibana_scan ;;
			5671) amqp_scan ;;
			5672) amqp_scan ;;
			5800) vnc_scan ;;
			5801) vnc_scan ;;
			5900) vnc_scan ;;
			5901) vnc_scan ;;
			5985) winrm_scan ;;
			5986) omi_scan ;;
			6000) x11_scan ;;
			8009) ajp_scan ;;
			8089) splunk_scan ;;
			8333) bitcoin_scan ;;
			18333) bitcoin_scan ;;
			38333) bitcoin_scan ;;
			18444) bitcoin_scan ;;
			9000) fastcgi_scan ;;
			9001) hsqldb_scan ;;
			9042) cassandra_scan ;;
			9160) cassabdra_scan ;;
			9100) raw_printing_scan ;;
			9200) elasticsearch_scan ;;
			10000) ndmp_scan ;;
			11211) memcached_scan ;;
			15672) rabbitmq_scan ;;
			24007) glusterfs_scan ;;
			24008) glusterfs_scan ;;
			24009) glusterfs_scan ;;
			49152) glusterfs_scan ;;
			44134) tiller_helm ;;
			44818) ethernetip_scan ;;
			47808) bacnet_scan ;;
			50030) hadoop_scan ;;
			50060) hadoop_scan ;;
			50070) hadoop_scan ;;
			50075) hadoop_scan ;;
			50090) hadoop_scan ;;
			80) http_scan ;;
			443) https_scan ;;
			1443) https_scan ;;
			*) ;;  # Default case if no specific action is needed for a port
		esac
	done
}

###############################################################
# Network                                                     #
###############################################################
default_nmap() {
	echo "[*] Starting full reconnaissance scan at ${current_time}"	

	echo " "

	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Performing full reconnaissance scan${ENDCOLOR}\n"
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_YELLOW}WARNING${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Sudo privileges is required to perform reconnaissance scan${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/
	path_check "$(pwd)/Netauto/$IP" "Created output directory of target live host under '$(pwd)/Netauto/$IP/'" "Failed to create output directory of target list host under '$(pwd)/Netauto/$IP/'"

	mkdir -p Netauto/$IP/DEFAULT
	path_check "$(pwd)/Netauto/$IP" "Created default NMAP output directory of target live host under '$(pwd)/Netauto/$IP/DEFAULT'" "Failed to create default NMAP output directory of target list host under '$(pwd)/Netauto/$IP/DEFAULT'"


        # Scans ports only and no host discovery
        # Scans for all ports available
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing port scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		nmap -Pn $IP -p- -oN Netauto/$IP/DEFAULT/port_scan.txt
	else 
		nmap $MINRATE -Pn $IP -p- -oN Netauto/$IP/DEFAULT/port_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/port_scan.txt" "Generated output data of port scan under '$(pwd)/Netauto/DEFAULT/port_scan.txt'" "Failed to generate output data of port scan under '$(pwd)/Netauto/DEFAULT/port_scan.txt'"

        echo " "

        # Runs "Script Scan" to discover additional information about the target host
        # This is to potentially identify vulnerabilities and/or misconfigurations
        # Scans vendor and service version for each open port (Service Version Detection)
        # Useful for attempting to identify outdated version of service
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing script scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		nmap -sC -sV $IP -oN Netauto/$IP/DEFAULT/script_scan.txt
	else 
		nmap $MINRATE -sC -sV $IP -oN Netauto/$IP/DEFAULT/script_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/script_scan.txt" "Generated output data of script scan under '$(pwd)/Netauto/DEFAULT/script_scan.txt'" "Failed to generate output data of script scan under '$(pwd)/Netauto/DEFAULT/script_scan.txt'"

        echo " "

        # Performs "TCP SYN Scan" by conducting TCP scan of target host by sending SYN packets
        # Also known as "Stealth Scan" as it is designed to be less intrusive and more discreet
        # Scans ports only and no host discovery
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing stealth scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		sudo nmap -sS -Pn -n $IP -oN Netauto/$IP/DEFAULT/stealth_scan.txt
	else 
		sudo nmap $MINRATE -sS -Pn -n $IP -oN Netauto/$IP/DEFAULT/stealth_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/stealth_scan.txt" "Generated output data of stealth scan under '$(pwd)/Netauto/DEFAULT/stealth_scan.txt'" "Failed to generate output data of stealth scan under '$(pwd)/Netauto/DEFAULT/stealth_scan.txt'"

        echo " "

        # Enables aggressive and/or advanced scanning techniques 
        # This involves such as OS/Version Detection, Script Scanning and Traceroute
        # This provides wealth of information about the target system 
        # Useful for detailed reconnaissane and vulnerability assessment
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing aggressive scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		nmap -p- -A $IP -oN Netauto/$IP/DEFAULT/aggressive_scan.txt
	else 
		nmap $MINRATE -p- -A $IP -oN Netauto/$IP/DEFAULT/aggressive_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/aggressive_scan.txt" "Generated output data of aggressive scan under '$(pwd)/Netauto/DEFAULT/aggressive_scan.txt'" "Failed to generate output data of aggressive scan under '$(pwd)/Netauto/DEFAULT/aggressive_scan.txt'"

        echo " "

        # Scans all standard ports to establish connection system call using TCP Connection Scan
        # runs all the scripts and checks on them
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing TCP scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		nmap -n -v -sT -A $IP -oN Netauto/$IP/DEFAULT/TCP_scan.txt
	else
		nmap $MINRATE -n -v -sT -A $IP -oN Netauto/$IP/DEFAULT/TCP_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/TCP_scan.txt" "Generated output data of TCP scan under '$(pwd)/Netauto/DEFAULT/TCP_scan.txt'" "Failed to generate output data of TCP scan under '$(pwd)/Netauto/DEFAULT/TCP_scan.txt'"
	
        echo " "

        # Run Vulnerability Script to perform NSE Vulnerability Scan
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Executing vulnerability scan on target live host $IP${ENDCOLOR}\n"
	if [ "$MINRATE" == "" ]; then
		nmap -sV --script vuln $IP -oN Netauto/$IP/DEFAULT/vulnerability_scan.txt
	else
		nmap $MINRATE -sV --script vuln $IP -oN Netauto/$IP/DEFAULT/vulnerability_scan.txt
	fi
	path_check "$(pwd)/Netauto/$IP/DEFAULT/vulnerability_scan.txt" "Generated output data of vulnerability scan under '$(pwd)/Netauto/DEFAULT/vulnerability_scan.txt'" "Failed to generate output data of vulnerability scan under '$(pwd)/Netauto/DEFAULT/vulnerability_scan.txt'"

        echo " "

	echo "[*] Ending full reconnaissance scan at ${current_time}"	
}

tcp_nmap() {
	echo "[*] Starting full TCP scan at ${current_time}"	

	echo " "

	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Performing full TCP scan${ENDCOLOR}\n"
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_YELLOW}WARNING${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Sudo privileges is required to perform TCP scan${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/DEFAULT/TCP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/TCP" "Created full TCP output directory of target live host under '$(pwd)/Netauto/$IP/TCP'" "Failed to create full TCP output directory of target live host under '$(pwd)/Netauto/$IP/TCP'"

	nmap -n -v -sT -p- -T5 -oN Netauto/$IP/DEFAULT/TCP/full_scan.txt $IP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/TCP/full_scan.txt" "Generated output data of full TCP scan under '$(pwd)/Netauto/DEFAULT/TCP/full_scan.txt'" "Failed to generate output data of full TCP scan under '$(pwd)/Netauto/DEFAULT/TCP/full_scan.txt'"


	sudo nmap -T4 -F -sV -sC -oN Netauto/$IP/DEFAULT/TCP/extended_scan.txt --reason --stats-every 5s -p- $IP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/TCP/extended_scan.txt" "Generated output data of extended TCP scan under '$(pwd)/Netauto/DEFAULT/TCP/extended_scan.txt'" "Failed to generate output data of extended TCP scan under '$(pwd)/Netauto/DEFAULT/TCP/extended_scan.txt'"

        echo " "

	echo "[*] Ending full TCP scan at ${current_time}"	
}

udp_nmap() {
	echo "[*] Starting full UDP scan at ${current_time}"	

	echo " "

	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Performing full UDP scan${ENDCOLOR}\n"
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_YELLOW}WARNING${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Sudo privileges is required to perform UDP scan${ENDCOLOR}\n"

	mkdir -p Netauto/$IP/DEFAULT/UDP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/UDP" "Generated full TCP output directory of target live host under '$(pwd)/Netauto/$IP/UDP'" "Failed to generate full TCP output directory of target live host under '$(pwd)/Netauto/$IP/UDP'"

        sudo nmap -n -v -sU -oN Netauto/$IP/DEFAULT/UDP/full_scan.txt $IP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/UDP/full_scan.txt" "Generated output data of full UDP scan under '$(pwd)/Netauto/DEFAULT/UDP/full_scan.txt'" "Failed to generate output data of full UDP scan under '$(pwd)/Netauto/DEFAULT/UDP/full_scan.txt'"

        echo ""

        sudo nmap -n -v -sU -p- -T5 -oN Netauto/$IP/DEFAULT/UDP/extended_scan.txt $IP
	path_check "$(pwd)/Netauto/$IP/DEFAULT/UDP/extended_scan.txt" "Generated output data of extended UDP scan under '$(pwd)/Netauto/DEFAULT/UDP/extended_scan.txt'" "Failed to generate output data of extended UDP scan under '$(pwd)/Netauto/DEFAULT/UDP/extended_scan.txt'"

        echo " "

	echo "[*] Ending full UPD scan at ${current_time}"	
}

###############################################################
# Help                                                        #
###############################################################
usage() {
        echo "Usage: Netauto -i <ip_address> [-u, --usage] [-h, --help] [-i, --ip] [-p, --ports] [-t, --timing] [-m, --mode] [-o, --output]"
}

help() {
        printf "${BOLD}Network Enumeration Automator with Nmap${ENDCOLOR}\n"
	echo " "
        echo "Usage: Netauto -i <ip_address> [-u, --usage] [-h, --help] [-i, --ip] [-p, --ports] [-t, --timing] [-m, --mode] [-o, --output]"
	echo " "
        echo "Options:"
        printf "  ${BOLD}-h, --help${ENDCOLOR}              	  Help & Usage\n"
        printf "  ${BOLD}-i, --ip${ENDCOLOR}              	  Scan IP address of the target device\n"
        printf "  ${BOLD}-p, --ports${ENDCOLOR}              	  Scan ports of the targets device\n"
        printf "  ${BOLD}-t, --timing${ENDCOLOR}              	  Set NMAP timing minrate option\n"
        printf "  ${BOLD}-m, --mode${ENDCOLOR}              	  Set type of scan to perform\n"
        printf "  ${BOLD}-o, --output${ENDCOLOR}              	  Sets the output destination for scan results\n"
        echo " "
        echo "Timing minrate options:"
        printf "  ${BOLD}-T0${ENDCOLOR}              	  Paranoid\n"
        printf "  ${BOLD}-T1${ENDCOLOR}              	  Sneaky\n"
        printf "  ${BOLD}-T2${ENDCOLOR}              	  Polite\n"
	printf "  ${BOLD}-T3${ENDCOLOR}              	  Normal (Default)\n"
        printf "  ${BOLD}-T4${ENDCOLOR}              	  Aggressive\n"
        printf "  ${BOLD}-T5${ENDCOLOR}              	  Insane\n"
	echo " "
	echo "Mode options: "
	printf " ${BOLD}recon${ENDCOLOR}			  Full Reconnaissance Scan\n"
	printf " ${BOLD}tcp${ENDCOLOR}			  TCP Scan\n"
	printf " ${BOLD}udp${ENDCOLOR}			  UDP Scan\n"
	printf " ${BOLD}script${ENDCOLOR}			  Script Scan\n"
	printf " ${BOLD}brute${ENDCOLOR}			  Brute Scan\n"
	printf " ${BOLD}vuln${ENDCOLOR}			  Vulnerability Scan\n"
	echo " "
	exit 1
}

###############################################################
# Arguments                                                   #
###############################################################
if [ $# -eq 0 ];
then 
        help
        exit 1
fi

IP=""
PORTS=""
MINRATE=""

while [[ "$#" -gt 0 ]]; do
    case $1 in
        -h|--help) help ;;
	-u|--usage) usage ;;
        -i|--ip) IP="$2"; shift ;;
        -p|--ports) PORTS="$2"; shift ;;
        -m|--minrate) MINRATE="$2"; shift ;;
	*) printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Unknown Parameter Passed: $1${ENDCOLOR}"; exit 1 ;;
    esac
    shift
done

# ===============================================================
# Checking Conditions...
# ===============================================================

# --------------------------------------------------------------
# IP
# --------------------------------------------------------------
if [ -z "$IP" ]; then
        printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Missing IP address. Please specify an IP using the -i option.${ENDCOLOR}"
        exit 1
fi

# --------------------------------------------------------------
# MINRATE
# --------------------------------------------------------------
if [[ -n "$MINRATE" ]]; then
	# Check if the user selected only one timing option
	if [ -z "$MINRATE" ]; then
		$MINRATE=3
	fi

	# Check if the input is an integer
	if ! [[ $MINRATE =~ ^[0-9]+$ ]]; then
	    printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Minrate Input must be an integer${ENDCOLOR}"
	    exit 1
	fi

	# Check if the input is a single character
	if [ $MINRATE -ne 1 ]; then
	    printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Minrate Input must be a single character${ENDCOLOR}"
	    exit 1
	fi

	# Check if the integer is not higher than 5
	if [ $MINRATE -gt 5 ]; then
	    printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Minrate Input must not be higher than 5${ENDCOLOR}"
	    exit 1
	fi
fi 

# --------------------------------------------------------------
# Printing output options...
# --------------------------------------------------------------
echo " "

printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Testing connection on target live host $IP${ENDCOLOR}\n"

test_connection_command=$(nmap -sn $IP)
if echo "$test_connection_command" | grep -q "Host is up"; then 
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${BOLD_WHITE}Target live host $IP is open${ENDCOLOR}\n"
else
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${BOLD_RED}ERROR${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Invalid target live host connection${ENDCOLOR}\n"
	exit 0
fi

if [[ -n "$PORTS" ]]; then
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Enumerating the following ports: $PORTS${ENDCOLOR}\n"
fi
if [[ -n "$MINRATE" ]]; then
	printf "${WHITE}[${ENDCOLOR}${CYAN}$current_time${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}[${ENDCOLOR}${GREEN}INFO${ENDCOLOR}${WHITE}]${ENDCOLOR} ${WHITE}Applying the following minrate timing option: $MINRATE${ENDCOLOR}\n"
fi

echo " "

if [ -z "$PORTS" ]; then
	default_nmap
        exit 1
else 
	handle_ports $PORTS
	exit 1
fi

